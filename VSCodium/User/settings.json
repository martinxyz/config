{
    "window.zoomLevel": 1,
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "workbench.enableExperiments": false,
    "workbench.settings.enableNaturalLanguageSearch": false,
    "update.enableWindowsBackgroundUpdates": false,
    "update.mode": "none",
    "extensions.autoCheckUpdates": false,
    "extensions.autoUpdate": false,
    "update.showReleaseNotes": false,
    "editor.scrollBeyondLastLine": false,
    "editor.codeLens": false,
    "editor.cursorSurroundingLines": 8,
    "vscode-neovim.neovimPath": "/usr/bin/nvim",
    "keyboard.dispatch": "keyCode",
    "vim.easymotion": true,
    "vim.incsearch": true,
    "vim.useSystemClipboard": true,
    "vim.useCtrlKeys": true,
    "vim.hlsearch": true,
    "window.menuBarVisibility": "toggle",
    "files.autoSave": "onFocusChange",
    "rust-client.engine": "rust-analyzer",
    "workbench.sideBar.location": "right",
    "typescript.npm": "/home/martin/.local/bin/npm",
    "svelte.language-server.runtime": "/home/martin/.local/bin/node",
    "rust-analyzer.diagnostics.disabled": [
        "macro-error", "unresolved-proc-macro"
    ],
    "rust-analyzer.updates.channel": "nightly",
    "svelte.language-server.runtime": "/usr/bin/node",
    "workbench.sideBar.location": "right",
    "vim.handleKeys": {
        "<C-a>": false,
        "<C-f>": false,
        "<C-s>": false,
        "<C-/>": true,
        "<C-d>": true,
        "<C-w>": false
    },
    "vim.normalModeKeyBindings": [
        {
            "before": [
                "t"
            ],
            "commands": [
                "workbench.action.quickOpen"
                
            ]
        },
        {
            "before": [
                "<Backspace>"
            ],
            "after": [
                "V"
                
            ]
        },
        {
            "before": [
                "q"
            ],
            "commands": [
                "workbench.action.quickOpenPreviousRecentlyUsedEditorInGroup"
            ]
        },
        {
            "before": [
                "["
            ],
            "commands": [
                "extension.vim_down",
                "scrollLineDown",
                "extension.vim_down",
                "scrollLineDown",
                "extension.vim_down",
                "scrollLineDown",
                "extension.vim_down",
                "scrollLineDown"
            ]
        },
        {
            "before": [
                "]"
            ],
            "commands": [
                "extension.vim_up",
                "scrollLineUp",
                "extension.vim_up",
                "scrollLineUp",
                "extension.vim_up",
                "scrollLineUp",
                "extension.vim_up",
                "scrollLineUp"
            ]
        },
        {
            "before": [
                "<C-/>"
            ],
            "commands": [
                "editor.action.commentLine",
                "extension.vim_down"
            ]
        },
        {
            "before": [
                "<C-e>"
            ],
            "commands": [
                "workbench.action.terminal.newInActiveWorkspace"
            ]
        },
        {
            "before": [
                "<C-t>"
            ],
            "commands": [
                "workbench.action.quickOpen"
            ]
        },
        {
            "before": [
                "<C-w>"
            ],
            "commands": [
                "workbench.action.closeActiveEditor"
            ]
        },
        {
            "before": [
                ";"
            ],
            "after": [
                "A",
                ";",
                "<Esc>"
            ]
        },
        {
            "before": [
                "<tab>"
            ],
            "commands": [
                "editor.action.reindentlines"
            ]
        }
    ],
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                ":"
            ],
            "commands": [
                "workbench.action.showCommands"
            ]
        }
    ],
    "vim.commandLineModeKeyBindingsNonRecursive": [],
    "vim.visualModeKeyBindings": []
}
